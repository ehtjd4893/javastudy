package practice;

import java.util.Scanner;

public class ex {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b =sc.nextInt();
		int hun_b = b / 100;
		int ten_b = (b % 100) / 10;
		int one_b = (b % 100) % 10;
		
		System.out.println((a * one_b) + "");
		System.out.println((a * ten_b) + "");
		System.out.println((a * hun_b) + "");
	}

}
