package quiz06;

public class BreadAndChange {
	private int bread;
	private int change;
	
	public BreadAndChange(int change) {
		this.bread = 0;
		this.change = change;
	}
	public int getBread() {
		return bread;
	}
	public void setBread(int bread) {
		this.bread = bread;
	}
	public int getChange() {
		return change;
	}
	public void setChange(int change) {
		this.change = change;
	}
	
	
}
