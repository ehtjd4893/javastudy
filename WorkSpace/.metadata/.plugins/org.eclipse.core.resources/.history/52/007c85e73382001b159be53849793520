package ex03_overloading;
/*
 * 오버로딩
 * 1. 이름이 같고 매개변수가 다른 메소드가 있음
 * 2. 결과타입은 오버로딩과 상관 없음.
 * */
public class Rectangle {
	int width, height;
	
	void set(int w, int h) {
		width = w;
		height = h;
	}
	void set(int w) {
		width = w;
		height = w;
	}
	int getArea() {
		int area = width * height;
		return area <= 0 ? 0 : area;
	}


}
